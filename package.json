  "dependencies": {
    "chai": "^4.1.2",
    "cucumber": "^5.0.1",
    "cucumber-json-reporter-to-html": "git+https://github.com/KarneyenkaDzmitry/cucumber-json-reporter-to-html.git",
    "gulp": "^3.9.1",
