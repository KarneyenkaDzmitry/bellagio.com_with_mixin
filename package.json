{
  "name": "protractor_js",
  "version": "1.0.0",
  "description": "first steps in testing with protractor framework",
  "main": "index.js",
  "scripts": {
    "wd-up": "webdriver-manager update",
    "wd-st": "webdriver-manager start",
    "test": "protractor ./configs/conf.js",
